{
  "name": "express-todo",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon ./src/index.ts",
    "db:migrate": "typeorm-ts-node-commonjs migration:run -d ./src/database/index.ts",
    "db:rollback": "typeorm-ts-node-commonjs migration:revert -d ./src/database/index.ts",
    "db:create-migration": "typeorm migration:create ./src/database/migrations/$NAME",
    "db:seed": "yarn ts-node src/database/seeds/index.ts"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.6.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.11",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.0.1",
    "glob": "^11.0.1",
    "ioredis": "^5.6.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.14.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.21"
  }
}
